/* 全局样式 */

/* 引入第三方字体图标库 */
@import "./styles/iconfont.wxss";

/* 微信小程序不支持通配符 */
page,
view,
text,
swiper,
swiper-item,
iamge,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

/* 主题颜色 变量实现  */

page {
    /* 定义主题颜色 */
    --themeColor:#eb4450;
    /* 定义统一字体大小 */
    font-size: 26rpx;
}
image{
    width: 100%;
}